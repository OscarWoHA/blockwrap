{
    "servers":[
        {
            "name":"Hub",
            "file":"spigot-latest.jar",

            "arguments":{
                "before": "-Xms2G -Xmx4G",
                "after": ""
            },

            "directory":"/root/servers/Hub/",
            "command":"java_jar"
        }
    ],

    "commands": {
        "java_jar": {
            "command": "java $before -jar $file $after"
        }
    },

    "application": {
        "default_server": "Hub",
        "live_view_default": true,
        "automatic_start_default": true
    }
}
